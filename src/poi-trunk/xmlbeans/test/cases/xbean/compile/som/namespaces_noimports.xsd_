<xs:schema
   xmlns:xs="http://www.w3.org/2001/XMLSchema"
   xmlns:nw="http://openuri.org/nameworld"
   xmlns:imp="http://openuri.org/namespaceimported"
   targetNamespace="http://openuri.org/nameworld"
   elementFormDefault="qualified">

    <!-- import with complete file path -->

    <xs:element name="NamespaceRootElem">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="NameRootChild" type="xs:string" />
            </xs:sequence>
        </xs:complexType>

    </xs:element>

</xs:schema>
