													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="S" type="xs:string" use="optional" fixed="1.2.276.0.76.3.1.1.5.1.10"/>
											<xs:attribute name="SN" type="xs:string" use="prohibited" fixed="Versichertenstatus KVK ST 10"/>
