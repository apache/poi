			<xs:attribute name="S" type="xs:string" use="optional" fixed="1.2.276.0.76.3.1.1.5.2"/>
			<xs:attribute name="SN" type="xs:string" use="prohibited" fixed="GO-Stammdatei der KV des entsprechenden Abrechnungsquartals"/>
		</xs:complexType>
